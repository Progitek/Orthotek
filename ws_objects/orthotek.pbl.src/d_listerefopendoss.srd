table(column=(type=char(102) updatewhereclause=yes name=patnom dbname="patnom" )
 column=(type=char(25) updatewhereclause=yes name=t_referents_ref_nom dbname="t_referents.ref_nom" dbalias=".ref_nom" )
 column=(type=char(25) updatewhereclause=yes name=t_referents_ref_prenom dbname="t_referents.ref_prenom" dbalias=".ref_prenom" )
 column=(type=char(50) updatewhereclause=yes name=sal dbname="sal" )
 column=(type=date updatewhereclause=yes name=patient_opendoss dbname="patient.opendoss" dbalias=".opendoss" )
 retrieve="SELECT   (patient.patient_nom + ', ' + patient.patient_prenom) as patnom,
         t_referents.ref_nom,
			t_referents.ref_prenom,
			(select salutation from t_salutation where id_sal = t_referents.id_sal) as sal,
			patient.opendoss
FROM     patient INNER JOIN t_referents ON (if :ra_reftype = 1 then patient.id_ref else patient.referant_nom endif ) = t_referents.id
WHERE    isnull(t_referents.actif,0) = 1 AND
			patient.no_ortho = :ra_no_ortho AND
         patient.opendoss BETWEEN :ra_debut AND :ra_fin AND
			(if :ra_reftype = 1 then patient.id_ref else patient.referant_nom endif ) > 0
ORDER BY t_referents.ref_nom ASC, t_referents.ref_prenom ASC" arguments=(("ra_no_ortho", number),("ra_debut", date),("ra_fin", date),("ra_reftype", number)) )
