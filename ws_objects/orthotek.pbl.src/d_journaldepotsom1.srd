 column=(type=char(30) updatewhereclause=yes name=ortho_id_dr_ville dbname="ortho_id.dr_ville" )
 column=(type=char(30) updatewhereclause=yes name=ortho_id_dr_province dbname="ortho_id.dr_province" )
 column=(type=char(15) updatewhereclause=yes name=ortho_id_dr_tel_bur dbname="ortho_id.dr_tel_bur" )
 column=(type=char(50) updatewhereclause=yes name=ortho_id_dr_adresse dbname="ortho_id.dr_adresse" )
 retrieve="  SELECT ortho_id.dr_nom_complet,   
         paiement.date_paiement,   
         paiement.montant_recu,   
         paiement.paiement,
			paiement.id_lot, 
         patient.no_ortho,   
         patient.patient_prenom,   
         patient.patient_nom,
			patient.id_satellite, 
         ortho_id.dr_compte,   
         ortho_id.dr_banque,   
         ortho_id.dr_ville,   
         ortho_id.dr_province,   
         ortho_id.dr_tel_bur,   
         ortho_id.dr_adresse  
    FROM traitements INNER JOIN patient ON traitements.patient_id = patient.patient_id
							INNER JOIN paiement ON traitements.traitement_id = paiement.traitement_patient_id,
			patient INNER JOIN ortho_id ON ortho_id.ortho_id = patient.no_ortho
   WHERE ortho_id.ortho_id = :ra_no_ortho AND
			paiement.date_paiement BETWEEN :ra_start AND :ra_end AND
         paiement.cache <> 1   
ORDER BY paiement.paiement ASC,   
         patient.patient_nom ASC,   
         patient.patient_prenom ASC   
" arguments=(("ra_no_ortho", number),("ra_start", date),("ra_end", date)) )
