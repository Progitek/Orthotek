 retrieve="SELECT   t_entente.entente_id,   
         t_entente.traitement,   
         t_entente.debut,   
         t_entente.fin,   
         t_entente.cout_total,   
         t_entente.cout_total1,    
         t_entente.diag_anal1,   
         t_entente.paiement_init1,   
         t_entente.paiement_fin1,    
         t_entente.mensualite1,    
         t_entente.duree1,
			t_entente.duree,  
			t_entente.datepaieinit,
         t_contact.prenom,   
         t_contact.nom,
         ortho_id.dr_nom_complet,
         ortho_id.dr_adresse,
         ortho_id.dr_ville,
         ortho_id.dr_code_postal,
         ortho_id.dr_province, 
         ortho_id.dr_tel_bur,  
         t_entente.patient_id  
FROM     t_entente INNER JOIN t_contact ON t_entente.id_contact = t_contact.id_contact,  
         t_contact INNER JOIN ortho_id ON t_contact.ortho_id = ortho_id.ortho_id   
WHERE    t_entente.entente_id = :ra_id
" arguments=(("ra_id", number)) )
