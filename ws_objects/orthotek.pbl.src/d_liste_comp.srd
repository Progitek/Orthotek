table(column=(type=long updatewhereclause=yes identity=yes name=patient_patient_id dbname="patient.patient_id" )
 column=(type=char(50) updatewhereclause=yes name=patient_patient_nom dbname="patient.patient_nom" )
 column=(type=char(50) updatewhereclause=yes name=patient_patient_prenom dbname="patient.patient_prenom" )
 column=(type=long updatewhereclause=yes name=patient_no_ortho dbname="patient.no_ortho" )
 column=(type=long updatewhereclause=yes name=patient_id_satellite dbname="patient.id_satellite" )
 column=(type=char(100) updatewhereclause=yes name=ortho_id_dr_nom_complet dbname="ortho_id.dr_nom_complet" )
 column=(type=decimal(2) updatewhereclause=yes name=totcouttrait dbname="totcouttrait" )
 column=(type=decimal(2) updatewhereclause=yes name=montantpaye dbname="montantpaye" )
 column=(type=decimal(2) updatewhereclause=yes name=chrgdujour dbname="chrgdujour" )
 column=(type=decimal(2) updatewhereclause=yes name=chrg dbname="chrg" )
 column=(type=date updatewhereclause=yes name=prchcpd dbname="prchcpd" )
 column=(type=long updatewhereclause=yes name=nbcheques dbname="nbcheques" )
 column=(type=decimal(2) updatewhereclause=yes name=totcheques dbname="totcheques" )
 retrieve="  SELECT DISTINCT patient.patient_id,
			patient.patient_nom,
			patient.patient_prenom,
			patient.no_ortho,
			patient.id_satellite,
			ortho_id.dr_nom_complet,
         (select isnull(sum(isnull(traitements.montant_original,0)),0) from traitements where traitements.patient_id = patient.patient_id) as totcouttrait,
			(select isnull(sum(isnull(montant_recu,0)),0) from paiement inner join traitements on paiement.traitement_patient_id = traitements.traitement_id where traitements.patient_id = patient.patient_id) as montantpaye,
			(select isnull(sum(isnull(t_rdv.chrgduree,0)),0) from t_rdv where patient.patient_id = t_rdv.patient_id and (t_rdv.rdvetat <> 'c' OR t_rdv.rdvetat is null) and t_rdv.rdvdate between :ra_debut and :ra_fin and
			(select addtocharge from type_de_traitement where ortho_id = patient.no_ortho and type_traitement_id = t_rdv.typetraitid) = 1) as chrgdujour,
			(select isnull(sum(isnull(t_rdv.chrgduree,0)),0) from t_rdv where t_rdv.patient_id = patient.patient_id	and t_rdv.rdvdate <= :ra_debut and (t_rdv.rdvetat <> 'c' OR t_rdv.rdvetat is null)) as chrg,
			(select min(date_cheque) from cheques_postdates inner join traitements on cheques_postdates.traitement_patient_id = traitements.traitement_id where traitements.patient_id = patient.patient_id and cheques_postdates.date_cheque > today()) as prchcpd,
			(select count(*) from cheques_postdates inner join traitements on cheques_postdates.traitement_patient_id = traitements.traitement_id where traitements.patient_id = patient.patient_id and date_cheque > today()) as nbcheques,
			(select isnull(sum(isnull(paiement,0)),0) from cheques_postdates inner join traitements on cheques_postdates.traitement_patient_id = traitements.traitement_id where traitements.patient_id = patient.patient_id and date_cheque > today()) as totcheques
    FROM patient INNER JOIN t_rdv r ON patient.patient_id = r.patient_id
					  INNER JOIN ortho_id ON patient.no_ortho = ortho_id.ortho_id
   WHERE patient.no_ortho = :ra_ortho and
			(r.rdvetat <> 'c' OR r.rdvetat is null) and
			r.rdvdate between :ra_debut and :ra_fin and
			(patient.archivedate is null or patient.archivedate > :ra_debut) and
			chrgdujour <> 0 and
			(patient.id_satellite = :ra_satellite OR :ra_satellite is null)
ORDER BY patient.patient_nom ASC,
			patient.patient_prenom ASC

" arguments=(("ra_ortho", number),("ra_debut", date),("ra_fin", date),("ra_satellite", number))  sort="compute_sat A patient_patient_nom A patient_patient_prenom A " )
