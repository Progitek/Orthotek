public subroutine uf_setpos ();long ll_pos, ll_imgconf

DECLARE listpos CURSOR FOR
	select pos, id_imgconf from t_imagingconf where ortho_id = :v_no_ortho;
	
OPEN listpos;
	
	FETCH listpos into :ll_pos, :ll_imgconf;
	
	DO WHILE SQLCA.SQLCode = 0
		
		CHOOSE CASE ll_imgconf
			CASE 1
				st_pos1.text = string(ll_pos)
			CASE 2
				st_pos2.text = string(ll_pos) 
			CASE 3
				st_pos3.text = string(ll_pos) 
			CASE 4
				st_pos4.text = string(ll_pos) 
			CASE 5
				st_pos5.text = string(ll_pos) 
			CASE 6
				st_pos6.text = string(ll_pos) 
			CASE 7
				st_pos7.text = string(ll_pos) 
			CASE 8
				st_pos8.text = string(ll_pos)
			CASE 9
				st_pos9.text = string(ll_pos)
		END CHOOSE
		
		FETCH listpos into :ll_pos, :ll_imgconf;
	LOOP
	

CLOSE listpos;

end subroutine

