text(band=detail alignment="0" text="par des traitements orthodontiques. Nous profitons donc de l'occasion pour vous fournir quelques renseignements supplémentaires" border="0" color="33554432" x="64" y="1108" height="56" width="2917" html.valueishtml="0"  name=t_3 visible="1"  font.face="Arial" font.height="-8" font.weight="400"  font.family="2" font.pitch="2" font.charset="0" background.mode="1" background.color="536870912" )
