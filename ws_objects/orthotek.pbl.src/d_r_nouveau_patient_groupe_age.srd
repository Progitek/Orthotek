table(column=(type=long updatewhereclause=yes name=patient_patient_id dbname="patient.patient_id" dbalias=".patient_id" )
 column=(type=long updatewhereclause=yes name=age dbname="age" )
 column=(type=long updatewhereclause=yes name=groupe_age dbname="groupe_age" )
 column=(type=decimal(2) updatewhereclause=yes name=sum_montant dbname="sum_montant" )
 retrieve="  SELECT patient.patient_id,
			
		( CAST(if date_naissance is null then
									  ''
			  else
				  year(today()) - year(date_naissance) - if month(today()) < month(date_naissance) then
																  1
																  else if month(today()) = month(date_naissance) and day(today()) < day(date_naissance) then
																	  1
																  else
																	  0
																  endif
															  endif
			  endif as INTEGER) )
	 as age,
	(CASE  
		WHEN age >= 0 AND age <= 8
			THEN 1
		WHEN age >= 9 AND age <= 11
			THEN 2
		WHEN age >= 12 AND age <= 18
			THEN 3
		WHEN age >= 19 AND age <= 30
			THEN 4
		WHEN age >= 31 AND age <= 40
			THEN 5
		WHEN age >= 41 AND age <= 50
			THEN 6
		WHEN age >= 51 AND age <= 60
			THEN 7
		WHEN age >= 61 AND age <= 70
			THEN 8
		WHEN age >= 71 AND age <= 80
			THEN 9
		WHEN age >= 81
			THEN 10
		ELSE 0
	END ) as groupe_age,
	sum(montant) as sum_montant
    FROM patient	LEFT JOIN traitements ON traitements.patient_id = patient.patient_id
						LEFT JOIN factures_traitements ON factures_traitements.traitement_id = traitements.traitement_id
						  
WHERE opendoss >= :ad_debut AND (opendoss <= :ad_fin OR :ad_fin is null)   AND 
(patient.no_ortho =:al_spec or :al_spec is null)
group by patient.patient_id, date_naissance
