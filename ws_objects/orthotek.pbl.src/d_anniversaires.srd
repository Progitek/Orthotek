 column=(type=date updatewhereclause=yes name=nextrdv dbname="nextrdv" )
 retrieve="  SELECT DISTINCT ortho_id.dr_nom_complet ,
         patient.patient_prenom ,
         patient.patient_nom ,
         patient.date_naissance,
         patient.new_dossier,
         patient.patient_id,
			patient.id_activite,
			patient.id_patstatus,
		   (SELECT min(rdvdate) FROM t_rdv WHERE rdvdate >= today() AND patient.patient_id = t_rdv.patient_id) AS nextrdv
    FROM patient INNER JOIN ortho_id ON ortho_id.ortho_id = patient.no_ortho
		           LEFT OUTER JOIN t_rdv ON patient.patient_id = t_rdv.patient_id
   WHERE ortho_id.ortho_id = :ra_no_ortho
ORDER BY patient.patient_nom ASC, 
         patient.patient_prenom ASC
          " arguments=(("ra_no_ortho", number)) )
