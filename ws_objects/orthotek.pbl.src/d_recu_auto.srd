 retrieve="  SELECT   paiement.recu,   
           paiement.date_paiement,   
           paiement.paiement_id,   
           patient.recu_automatique,
			  patient.langue  
    FROM   traitements  INNER JOIN paiement ON paiement.traitement_patient_id = traitements.traitement_id
           					INNER JOIN patient  ON patient.patient_id = traitements.patient_id      
   WHERE   paiement.date_paiement BETWEEN :ra_datedebut AND :ra_datefin AND  
           patient.recu_automatique = '1'  AND  
           paiement.recu <> '1' AND
			  paiement.montant_recu > 0
ORDER BY   patient.patient_nom ASC,
			  patient.patient_prenom ASC" update="paiement" updatewhere=0 updatekeyinplace=no arguments=(("ra_datedebut", date),("ra_datefin", date)) )
