public subroutine sauve ()
public subroutine uf_setcolorpalette ()
end prototypes

public subroutine f_color (long fl_color, string fs_nomsys);integer li_exist,li_newrow

select count(*) into :li_exist from t_color where color = :fl_color and ortho_id = :v_no_ortho;
if li_exist > 0 then
	error_type(139)
else
	ii_modeajout = 1
	li_newrow = dw_gestioncouleur.insertrow(0)
	dw_gestioncouleur.scrolltorow(li_newrow)
	dw_gestioncouleur.setitem(li_newrow,'color',fl_color)
	dw_gestioncouleur.setitem(li_newrow,'name',fs_nomsys)
	dw_gestioncouleur.setitem(li_newrow,'ortho_id',v_no_ortho)
	sauve()
//	f_selectcolor()
end if
end subroutine

public subroutine sauve ();if dw_gestioncouleur.update() = 1 then
   commit using SQLCA;
	ii_modeajout = 0
	change = false
else
	error_type(50)
	rollback using SQLCA;
end if
end subroutine

public subroutine uf_setcolorpalette ();long ll_color,ll_col = 0

Declare colorpalette Cursor For
 
 select color from t_color;
           			
OPEN colorpalette;

Do While SQLCA.SQLCode = 0
	ll_col++
   Fetch colorpalette Into :ll_color;
