 retrieve="select  	ortho_id.ortho_id,
        	ortho_id.dr_nom_complet,   
        	ortho_id.dr_ville_province_code_postale,   
        	ortho_id.dr_tel_bur,   
        	ortho_id.dr_adresse,
        	ortho_id.dr_numero,
			ortho_id.tps,
			ortho_id.tvq,
			ortho_id.dr_special,
			ortho_id.cie,
        	patient.patient_nom,   
        	patient.patient_prenom,
			patient.prochain_rappel,
			patient.recuresp,
			patient.nomresp1,
			patient.langue,
			isnull(patient.new_dossier,'') as new_dossier,
        	t_salutation.salutation,
        	isnull(sum(isnull(t_recu.montantrecu,0)),0) as montantrecu,
        	isnull(sum(isnull(t_recu.idrecu,0)),0) as idrecu,   
        	min(t_recu.daterecu) as mindaterecu,
			max(t_recu.daterecu) as maxdaterecu,      
	      min(t_recu.recurecu) as recurecu,  
			min(t_recu.recunote) as recunote,
        	if :ra_idcontact is null then    
        		t_salutation.salutation || ' ' || patient.patient_prenom ||' ' ||	patient.patient_nom
			else 
				salcont.salutation || ' ' || t_contact.prenom || ' ' || t_contact.nom
			endif as recucontact,
			min(t_recu.recutaxe) as recutaxe,
			count(t_recu.idrecu) as nbrecu,      
        	patient.patient_id,
			t_options.recure as re,
			isnull(t_options.recudelassnumber, 0) as hidenumber,
        	isnull(patient.aposter,0) as poste,
			isnull(t_options.sigrecu,0) as signature,
			isnull(phasetype,'f') as phasetype,
  			t_options.noterecu,
			t_options.noterecuan,
			t_options.signfile,
			ifnull(t_contact.id_contact,patient.adresse,t_contact.adresse) as cadresse,
			ifnull(t_contact.id_contact,patient.ville,t_contact.ville) as ville,
			ifnull(t_contact.id_contact,patient.province,t_contact.province) as province,
			ifnull(t_contact.id_contact,patient.code_postale,t_contact.zip) as codepostal
from
(select  montant_recu as montantrecu,
        	paiement_id as idrecu,
        	date_paiement as daterecu,
        	nomresp as resprecu,
        	recu as recurecu,
        	recunote as recunote,
        	id_contact as recucontact,
        	taxe as recutaxe,
        	traitement_patient_id as idtraitementrecu,
        	'P' as typetrans
from    	paiement
union
select  	paiement,
        	cheques_postdates_id,
        	date_cheque,
        	nomresp,
        	recu,
        	'',
        	id_contact,
        	taxe,
        	traitement_patient_id,
        	'C'
from    	cheques_postdates) as t_recu 	INNER JOIN traitements ON traitements.traitement_id = t_recu.idtraitementrecu
                                     	INNER JOIN patient ON patient.patient_id = traitements.patient_id
                                     	INNER JOIN ortho_id ON ortho_id.ortho_id = patient.no_ortho
                                     	INNER JOIN t_options ON t_options.ortho_id = ortho_id.ortho_id
                                     	LEFT OUTER JOIN t_contact ON t_contact.id_contact = t_recu.recucontact
				                     		LEFT OUTER JOIN t_salutation ON t_salutation.id_sal = patient.id_sal
													LEFT OUTER JOIN t_salutation as salcont ON salcont.id_sal = t_contact.id_sal 
where   	t_recu.typetrans = :ra_typetrans AND
        	t_recu.idrecu IN (:ra_idrecu) AND
			(t_recu.recucontact = :ra_idcontact or :ra_idcontact is null)
group by ortho_id.ortho_id,
        	ortho_id.dr_nom_complet,   
        	ortho_id.dr_ville_province_code_postale,   
        	ortho_id.dr_tel_bur,   
        	ortho_id.dr_adresse,
        	ortho_id.dr_numero,
			ortho_id.tps,
			ortho_id.tvq,
			ortho_id.dr_special,
			ortho_id.cie,
        	patient.patient_nom,   
        	patient.patient_prenom,
			patient.adresse,
			patient.ville,
			patient.province,
			patient.code_postale,
			patient.prochain_rappel,
			patient.recuresp,
			patient.nomresp1,
			patient.langue,
			patient.new_dossier,
        	t_salutation.salutation,
			patient.patient_id,
			re,
			hidenumber,
        	poste,
			signature,
			phasetype,
  			t_options.noterecu,
			t_options.noterecuan,
			t_options.signfile,
			t_contact.adresse,
			t_contact.ville,
			t_contact.province,
			t_contact.zip,
			t_contact.id_contact,
			t_contact.nom,
			t_contact.prenom,
			salcont.salutation" arguments=(("ra_typetrans", string),("ra_idrecu", numberlist),("ra_idcontact", number)) )
