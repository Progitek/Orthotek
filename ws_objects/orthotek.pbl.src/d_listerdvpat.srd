 retrieve="  SELECT t_rdv.rdvdate,
			t_rdv.rdvheure,
			t_rdv.rdvetat,
			(select nom_traitement from type_de_traitement where type_traitement_id = typetraitid and ortho_id = :ra_ortho) as traitnom,
			(select patient_nom + ', ' + patient_prenom from patient where patient.patient_id = t_rdv.patient_id) as patnom,
			(select no_ortho from patient where patient.patient_id = t_rdv.patient_id) as orthoid
    FROM t_rdv
	WHERE orthoid = :ra_ortho AND
			t_rdv.rdvdate BETWEEN :ra_start AND :ra_end
ORDER BY t_rdv.rdvdate ASC,
			t_rdv.rdvheure ASC" arguments=(("ra_ortho", number),("ra_start", date),("ra_end", date)) )
