text(band=detail alignment="0" text="2) S'abstenir de mâcher de la gomme ou de manger des aliments collants, car ils peuvent faire décimenter les bagues" border="0" color="33554432" x="320" y="1756" height="56" width="2629" html.valueishtml="0"  name=t_10 visible="1"  font.face="Arial" font.height="-8" font.weight="400"  font.family="2" font.pitch="2" font.charset="0" background.mode="1" background.color="536870912" )
text(band=detail alignment="0" text="Les bagues et les appareils n'endommagent pas les dents pourvu que l'hygiène dentaire soit IMPECCABLE." border="0" color="33554432" x="288" y="1444" height="56" width="2629" html.valueishtml="0"  name=t_7 visible="1"  font.face="Arial" font.height="-8" font.weight="400"  font.family="2" font.pitch="2" font.charset="0" background.mode="1" background.color="536870912" )
text(band=detail alignment="0" text="La bonne conduite d'un traitement d'orthodontie et l'établissement d'honoraires pour ce travail se font en prenant pour" border="0" color="33554432" x="288" y="1276" height="56" width="2629" html.valueishtml="0"  name=t_5 visible="1"  font.face="Arial" font.height="-8" font.weight="400"  font.family="2" font.pitch="2" font.charset="0" background.mode="1" background.color="536870912" )
text(band=detail alignment="0" text="L'analyse détaillée des données diagnostiques nous révèle que la malocclusion dentaire peut-être grandement améliorée" border="0" color="33554432" x="288" y="1048" height="56" width="2647" html.valueishtml="0"  name=t_2 visible="1"  font.face="Arial" font.height="-8" font.weight="400"  font.family="2" font.pitch="2" font.charset="0" background.mode="1" background.color="536870912" )
