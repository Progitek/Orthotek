 retrieve="  SELECT patient.patient_nom,   
         patient.patient_prenom,   
         patient.date_naissance,   
         patient.no_dossier,   
         patient.adresse,   
         patient.tel_maison,   
         patient.tel_bureau,   
         patient.ville,   
         patient.province,   
         patient.code_postale,   
         patient.nom_mere,   
         patient.no_tel_mere,   
         patient.nom_pere,   
         patient.no_tel_pere,
			(select salutation from t_salutation where patient.id_sal = t_salutation.id_sal) as salutation
    FROM patient  
   WHERE patient.patient_id in (:ra_patient)   
	ORDER BY patient.patient_nom ASC,
	         patient.patient_prenom ASC
" arguments=(("ra_patient", numberlist)) )
