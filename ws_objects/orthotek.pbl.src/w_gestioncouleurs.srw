event clicked;long ll_idcolor,ll_nbrow
ll_idcolor = dw_gestioncouleur.getitemnumber(dw_gestioncouleur.getRow(),'color_id')
select count(*) into :ll_nbrow from type_de_traitement where color_id = :ll_idcolor;
if ll_nbrow = 0 then
	dw_gestioncouleur.deleterow(dw_gestioncouleur.getRow())
	sauve()
//	f_selectcolor()
else
	error_type(143)
end if
end event

