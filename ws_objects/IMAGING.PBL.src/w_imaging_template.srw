event itemchanged;string item, name
long ll_pos

if dwo.name = "img" then
	
	
	this.accepttext()
	this.update()
	
	item = this.getitemstring(1, "img")
	
	name = this.classname()
	
	choose case name
			
			case "dw_1"
			ddlb_1.SelectItem(item, 0)

			case "dw_2"
			ddlb_2.SelectItem(item, 0)
			
			case "dw_3"
			ddlb_3.SelectItem(item, 0)
			
			case "dw_4"
			ddlb_4.SelectItem(item, 0)
			
			case "dw_5"
			ddlb_5.SelectItem(item, 0)
			
			case "dw_6"
			ddlb_6.SelectItem(item, 0)
			
			case "dw_7"
			ddlb_7.SelectItem(item, 0)
			
			case "dw_8"
			ddlb_8.SelectItem(item, 0)
			
		
	end choose
	
end if

if dwo.name = 'pos' then
	ll_pos = long(data)
	update t_imagingconf set pos = null where pos = :ll_pos and ortho_id = :v_no_ortho;
	if error_type(-1) = 1 then
		commit using SQLCA;
	else
		rollback using SQLCA;
	end if
	
	this.accepttext()
	this.update()
	
	uf_setpos()
	
end if
end event

