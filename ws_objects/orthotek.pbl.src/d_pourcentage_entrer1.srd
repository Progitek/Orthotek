 column=(type=decimal(2) updatewhereclause=yes name=traitements_montant_paye dbname="traitements.montant_paye" )
 column=(type=long updatewhereclause=yes key=yes name=t_pourcentages_traitement_id dbname="t_pourcentages.traitement_id" )
 column=(type=char(30) updatewhereclause=yes name=dr_nom_complet dbname="ortho_id.dr_nom_complet" )
 column=(type=long update=yes updatewhereclause=yes name=pourcentage dbname="t_pourcentages.pourcentage" )
 column=(type=long updatewhereclause=yes name=year1 dbname="year1" )
 column=(type=long updatewhereclause=yes name=year2 dbname="year2" )
 column=(type=decimal(2) updatewhereclause=yes name=totfact1 dbname="totfact1" )
 column=(type=decimal(2) updatewhereclause=yes name=totfact dbname="totfact" )
 column=(type=long updatewhereclause=yes key=yes name=annee dbname="t_pourcentages.annee" )
 column=(type=long updatewhereclause=yes name=maxpourc dbname="maxpourc" )
 retrieve="  SELECT patient.patient_id,
			patient.patient_nom,   
         patient.patient_prenom,
			patient.new_dossier,
			patient.id_satellite,
         traitements.debut_traitement,
         traitements.fin_traitement, 
			traitements.montant_original,
			traitements.montant_paye,  
         t_pourcentages.traitement_id, 
			ortho_id.dr_nom_complet,
			t_pourcentages.pourcentage,
			(select pourcentage from t_pourcentages where annee = :ra_year - 1 and traitement_id = traitements.traitement_id) as year1,
			(select pourcentage from t_pourcentages where annee = :ra_year - 2 and traitement_id = traitements.traitement_id) as year2,
			(select sum(montant) from factures_traitements where factures_traitements.traitement_id = traitements.traitement_id) as totfact1,
			(if totfact1 is null then 0 else totfact1 endif) as totfact,
			t_pourcentages.annee,
			(select max(isnull(pourcentage,0)) from t_pourcentages where annee < :ra_year and traitement_id = traitements.traitement_id) as maxpourc
    FROM traitements INNER JOIN patient  ON traitements.patient_id = patient.patient_id
							INNER JOIN t_pourcentages ON t_pourcentages.traitement_id = traitements.traitement_id,
			patient 		INNER JOIN ortho_id ON patient.no_ortho = ortho_id.ortho_id
   WHERE patient.no_ortho = :ra_ortho and
			t_pourcentages.annee = :ra_year and
			totfact <> 0 and
			(maxpourc <> 100 or maxpourc is null)
			
ORDER BY patient.patient_nom ASC,   
         patient.patient_prenom ASC" update="t_pourcentages" updatewhere=0 updatekeyinplace=no arguments=(("ra_ortho", number),("ra_year", number)) )
