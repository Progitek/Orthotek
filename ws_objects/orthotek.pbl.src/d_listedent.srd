 column=(type=char(30) updatewhereclause=yes name=t_dentists_det_nom dbname="t_dentists.det_nom" dbalias=".det_nom" )
 column=(type=char(30) updatewhereclause=yes name=t_dentists_det_prenom dbname="t_dentists.det_prenom" dbalias=".det_prenom" )
 column=(type=char(50) updatewhereclause=yes name=sal dbname="sal" dbalias=".sal" )
 column=(type=date updatewhereclause=yes name=datetrait dbname="datetrait" )
 retrieve="SELECT   (patient.patient_nom + ', ' + patient.patient_prenom) as patnom,
			patient.patient_nom,
         t_dentists.det_nom,
			t_dentists.det_prenom,
			(select salutation from t_salutation where id_sal = t_dentists.id_sal) as sal,
			(select min(traitements.debut_traitement) from traitements where patient.patient_id = traitements.patient_id) as datetrait
FROM     patient INNER JOIN t_dentists ON patient.id_dentist = t_dentists.id_dentist
WHERE    t_dentists.actif = 1 AND
			patient.no_ortho = :ra_no_ortho AND
         datetrait BETWEEN :ra_debut AND :ra_fin AND
			patient.id_dentist > 0
ORDER BY t_dentists.det_nom ASC, t_dentists.det_prenom ASC" arguments=(("ra_no_ortho", number),("ra_debut", date),("ra_fin", date)) )
