 retrieve="  SELECT traitements.phase,   
         traitements.debut_traitement,   
         traitements.fin_traitement,   
         traitements.traitement_id as trait, 
			traitements.traitement_id,  
         traitements.pourcentage_actuel,   
         traitements.montant_original,   
         traitements.montant_du,   
         traitements.montant_paye,   
         traitements.montant_paye_pourc,   
         traitements.note,   
         traitements.patient_id,
			traitements.frais,
			traitements.id_phase,
			traitements.phasetype,
			traitements.etatcompte,
			:ra_idcont as idcont,
			(select isnull(SUM(isnull(montant,0)),0) from factures_traitements where traitement_id = trait and id_contact = :ra_idcont) as v_frais,
			(select isnull(SUM(isnull(montant,0)),0) from factures_traitements where traitement_id = trait and date_facture <= today() and id_contact = :ra_idcont) as v_sum,
 			(select isnull(sum(isnull(montant_recu,0)),0) from paiement where traitement_patient_id = trait and id_contact = :ra_idcont) as montantpaye,
			(select isnull(sum(isnull(paiement,0)),0) from cheques_postdates where traitement_patient_id = trait and id_contact = :ra_idcont and (indicateur <> '1' or indicateur is null)) as totcpd
	 FROM traitements 
   WHERE traitements.traitement_id = :ra_traitid  
  
" update="traitements" updatewhere=0 updatekeyinplace=no arguments=(("ra_traitid", number),("ra_idcont", number)) )
