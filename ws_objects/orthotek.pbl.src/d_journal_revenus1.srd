 column=(type=decimal(2) updatewhereclause=yes name=compute_0002 dbname="compute_0002" )
 retrieve="  SELECT distinct ~"factures_traitements~".~"date_facture~",   
         sum(~"factures_traitements~".~"montant~")  
    FROM ~"factures_traitements~",   
         ~"traitements~",   
         ~"patient~",   
         ~"ortho_id~"  
   WHERE ( ~"factures_traitements~".~"traitement_id~" = ~"traitements~".~"traitement_id~" ) and  
         ( ~"traitements~".~"patient_id~" = ~"patient~".~"patient_id~" ) and  
         ( ~"patient~".~"no_ortho~" = ~"ortho_id~".~"ortho_id~" ) and  
         ( ( ~"ortho_id~".~"ortho_id~" = :ra_ortho_id ) AND  
         ( ~"factures_traitements~".~"date_facture~" >= :ra_debut ) AND  
         ( ~"factures_traitements~".~"date_facture~" <= :ra_fin ) ) AND  
         ~"factures_traitements~".~"cache~" = 0   
GROUP BY ~"factures_traitements~".~"date_facture~"  
ORDER BY ~"factures_traitements~".~"date_facture~" ASC   
" arguments=(("ra_ortho_id", number),("ra_debut", date),("ra_fin", date)) )
