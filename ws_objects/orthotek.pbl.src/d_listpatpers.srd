 column=(type=time updatewhereclause=yes name=t_rdv_rdvheure dbname="t_rdv.rdvheure" dbalias=".rdvheure" )
 retrieve="select  patient.patient_nom,
        patient.patient_prenom,
        t_personnels.nom,
        t_personnels.prenom,
		  t_personnels.id_personnel,
        t_rdv.rdvheure
from t_rdv  INNER JOIN patient ON patient.patient_id = t_rdv.patient_id
            INNER JOIN t_personnels ON patient.id_personnel = t_personnels.id_personnel
where t_rdv.rdvdate = :ra_date and
		t_rdv.ortho_id = :ra_orthoid
		
order by nom asc, prenom asc, rdvheure asc" arguments=(("ra_date", date),("ra_orthoid", number)) )
