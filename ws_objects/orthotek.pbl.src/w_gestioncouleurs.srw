	cb_delete.Text = "Delete"
end if
end event

type dw_gestioncouleur from u_dw within w_gestioncouleurs
integer y = 84
integer width = 1934
integer height = 1832
integer taborder = 10
string title = "gestioncouleur"
string dataobject = "d_gestioncouleur"
boolean ib_isupdateable = false
boolean ib_rmbmenu = false
boolean ib_rmbfocuschange = false
end type

event constructor;if v_langue = 'an' then
	this.object.t_preview.text = 'Preview'
	this.object.t_hor.text = 'Sq.'
	this.object.t_spec.text = 'Spec.'
	this.object.t_hyg.text = 'Ass.'
end if
dw_gestioncouleur.SetTransObject(SQLCA)
dw_gestioncouleur.setrowfocusindicator(hand!)
dw_gestioncouleur.retrieve(v_no_ortho)
end event

event updateend;long ll_row, ll_colorid
integer test
if rowsinserted <> 0 then
	ll_row = dw_gestioncouleur.getrow()
	select max(color_id) into :ll_colorid from t_color;
	dw_gestioncouleur.setitem(ll_row,'color_id',ll_colorid)
	if ii_modeajout = 1 then   
     	test = dw_gestioncouleur.setitemstatus(ll_row,'color_id',primary!,notmodified!)
		test = dw_gestioncouleur.setitemstatus(ll_row,'color',primary!,notmodified!)
		test = dw_gestioncouleur.setitemstatus(ll_row,'name',primary!,notmodified!)
	end if
end if
end event

event editchanged;change = true
end event

event itemchanged;change = true
end event

event dberror;str_error  ErrorStr
//populate the error structure
ErrorStr.sqldbcode = sqldbcode
ErrorStr.sqlerrtext = sqlerrtext
ErrorStr.sqlsyntax = sqlsyntax
ErrorStr.buffer = buffer
ErrorStr.row = row

//open the error window
OpenWithParm ( w_error , ErrorStr )
//do not display the error message
RETURN 1
end event

event clicked;scrolltorow(getclickedrow())
end event

