 retrieve="  SELECT ortho_id.ortho_id,   
         ortho_id.dr_nom_complet,   
         ortho_id.dr_ville_province_code_postale,   
         ortho_id.dr_tel_bur,   
         ortho_id.dr_adresse,
         ortho_id.dr_numero,   
         patient.patient_nom,
			patient.nomresp1,  
			patient.nomresp2,
			patient.nomresp3, 
         patient.patient_prenom, 
			paiement.nomresp,  
         paiement.montant_recu,   
         paiement.date_debut_recu,   
         paiement.paiement_id,   
         paiement.date_paiement,   
         patient.prochain_rappel,   
         paiement.recu,
			paiement.recunote,   
         paiement.somme_recu,
         paiement.id_contact,    
         traitements.note,   
         patient.patient_id
    FROM traitements 
			INNER JOIN paiement ON traitements.traitement_id = paiement.traitement_patient_id
			INNER JOIN patient ON traitements.patient_id = patient.patient_id,
         patient INNER JOIN ortho_id ON patient.no_ortho = ortho_id.ortho_id
   WHERE paiement.paiement_id = :ra_paiement     
" update="paiement" updatewhere=0 updatekeyinplace=no arguments=(("ra_paiement", number)) )
