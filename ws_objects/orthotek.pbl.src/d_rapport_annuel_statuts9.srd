 column=(type=long updatewhereclause=yes name=compute_0002 dbname="compute_0002" )
 retrieve="  SELECT ~"t_status~".~"code~",   
         count(~"factures_traitements~".~"status_id~")  
    FROM ~"factures_traitements~",   
         ~"t_status~",   
         ~"traitements~",   
         ~"patient~"  

   WHERE ( ~"factures_traitements~".~"status_id~" = ~"t_status~".~"status_id~" ) and  
         ( ~"factures_traitements~".~"traitement_id~" = ~"traitements~".~"traitement_id~" ) and  
         ( ~"traitements~".~"patient_id~" = ~"patient~".~"patient_id~" ) and  
         ( ( ~"patient~".~"no_ortho~" = :ra_ortho_id ) AND  
         ( year(~"factures_traitements~".~"date_facture~") = :ra_year ) and
			( month(~"factures_traitements~".~"date_facture~") = 9 ) )
   
GROUP BY ~"t_status~".~"code~",  
			~"factures_traitements~".~"status_id~"" arguments=(("ra_ortho_id", number),("ra_year", number)) )
