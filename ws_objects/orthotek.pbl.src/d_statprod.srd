 retrieve="  SELECT DISTINCT (select sum(montant_original) from traitements INNER JOIN patient ON traitements.patient_id = patient.patient_id where patient.no_ortho = :ra_orthoid and year(debut_traitement) = :ra_year and month(debut_traitement) = 1) as janvier1,
			(if janvier1 is null then 0 else janvier1 endif) as janvier,
			(select sum(montant_original) from traitements INNER JOIN patient ON traitements.patient_id = patient.patient_id where patient.no_ortho = :ra_orthoid and year(debut_traitement) = :ra_year and month(debut_traitement) = 2) as fevrier1,
         (if fevrier1 is null then 0 else fevrier1 endif) as fevrier,
			(select sum(montant_original) from traitements INNER JOIN patient ON traitements.patient_id = patient.patient_id where patient.no_ortho = :ra_orthoid and year(debut_traitement) = :ra_year and month(debut_traitement) = 3) as mars1,
         (if mars1 is null then 0 else mars1 endif) as mars,
			(select sum(montant_original) from traitements INNER JOIN patient ON traitements.patient_id = patient.patient_id where patient.no_ortho = :ra_orthoid and year(debut_traitement) = :ra_year and month(debut_traitement) = 4) as avril1,
         (if avril1 is null then 0 else avril1 endif) as avril,
			(select sum(montant_original) from traitements INNER JOIN patient ON traitements.patient_id = patient.patient_id where patient.no_ortho = :ra_orthoid and year(debut_traitement) = :ra_year and month(debut_traitement) = 5) as mai1,
         (if mai1 is null then 0 else mai1 endif) as mai,
			(select sum(montant_original) from traitements INNER JOIN patient ON traitements.patient_id = patient.patient_id where patient.no_ortho = :ra_orthoid and year(debut_traitement) = :ra_year and month(debut_traitement) = 6) as juin1,
         (if juin1 is null then 0 else juin1 endif) as juin, 
			(select sum(montant_original) from traitements INNER JOIN patient ON traitements.patient_id = patient.patient_id where patient.no_ortho = :ra_orthoid and year(debut_traitement) = :ra_year and month(debut_traitement) = 7) as juillet1,
         (if juillet1 is null then 0 else juillet1 endif) as juillet,
			(select sum(montant_original) from traitements INNER JOIN patient ON traitements.patient_id = patient.patient_id where patient.no_ortho = :ra_orthoid and year(debut_traitement) = :ra_year and month(debut_traitement) = 8) as aout1,
         (if aout1 is null then 0 else aout1 endif) as aout,
			(select sum(montant_original) from traitements INNER JOIN patient ON traitements.patient_id = patient.patient_id where patient.no_ortho = :ra_orthoid and year(debut_traitement) = :ra_year and month(debut_traitement) = 9) as septembre1,
         (if septembre1 is null then 0 else septembre1 endif) as septembre,
			(select sum(montant_original) from traitements INNER JOIN patient ON traitements.patient_id = patient.patient_id where patient.no_ortho = :ra_orthoid and year(debut_traitement) = :ra_year and month(debut_traitement) = 10) as octobre1,
         (if octobre1 is null then 0 else octobre1 endif) as octobre,
			(select sum(montant_original) from traitements INNER JOIN patient ON traitements.patient_id = patient.patient_id where patient.no_ortho = :ra_orthoid and year(debut_traitement) = :ra_year and month(debut_traitement) = 11) as novembre1,
         (if novembre1 is null then 0 else novembre1 endif) as novembre,
			(select sum(montant_original) from traitements INNER JOIN patient ON traitements.patient_id = patient.patient_id where patient.no_ortho = :ra_orthoid and year(debut_traitement) = :ra_year and month(debut_traitement) = 12) as decembre1,
   		(if decembre1 is null then 0 else decembre1 endif) as decembre,
         ortho_id.dr_nom_complet,
         ortho_id.dr_adresse,
         ortho_id.dr_ville,
         ortho_id.dr_code_postal,
         ortho_id.dr_province,
         ortho_id.dr_tel_bur 
	FROM  patient INNER JOIN traitements ON patient.patient_id = traitements.patient_id
                 INNER JOIN ortho_id ON patient.no_ortho = ortho_id.ortho_id
	WHERE ortho_id = :ra_orthoid" arguments=(("ra_year", number),("ra_orthoid", number)) )
