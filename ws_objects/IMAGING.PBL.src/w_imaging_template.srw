event clicked;string ls_query, ls_img
string ls_dejala = "false"
string lsa_img []
int j

int i = 0

ls_query = "select img from t_imagingconf where ortho_id = " + string(v_no_ortho)


declare dropdown_cursor DYNAMIC CURSOR FOR SQLSA;
prepare sqlsa from :ls_query;
open dropdown_cursor;

fetch dropdown_cursor into :ls_img;

do while SQLCA.SQLcode = 0
	
	for j=1 to i 
		if lsa_img[j] = ls_img then
			ls_dejala = "true"
		end if
		
	next
	i++
	lsa_img[i] = ls_img 
	
	fetch dropdown_cursor into :ls_img;
loop

close dropdown_cursor;


if ls_dejala = "true" then
	if gs_langue = "an" THEN
		messagebox("Warning", "A picture type is used twice.")
	ELSE
		messagebox("Avertissement", "Une case photo est utilisée en double.")	
	END IF
else
	If IsValid(w_imaging) THEN
		w_imaging.trigger event get_image()
		w_imaging.uf_montage()
	END IF
	close(parent)
end if
end event

