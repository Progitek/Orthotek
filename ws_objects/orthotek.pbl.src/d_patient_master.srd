 column=(type=date updatewhereclause=yes name=date_naissance dbname="patient.date_naissance" )
 column=(type=char(10) updatewhereclause=yes name=new_dossier dbname="patient.new_dossier" )
 column=(type=char(20) updatewhereclause=yes name=langue dbname="patient.langue" )
