text(band=detail alignment="0" text="que votre compte soit crédité convenablement)." border="0" color="33554432" x="69" y="4004" height="56" width="1047" html.valueishtml="0"  name=t_38 visible="1"  font.face="Arial" font.height="-8" font.weight="400"  font.family="2" font.pitch="2" font.charset="0" background.mode="1" background.color="536870912" )
