 retrieve="  SELECT patient.patient_nom,   
         patient.patient_prenom,   
         patient.date_naissance,   
         ortho_id.dr_nom_complet,   
         patient.adresse,   
         patient.tel_maison,   
         patient.tel_bureau,   
         patient.ville,   
         patient.province,   
         patient.code_postale,   
         patient.nom_mere,   
         patient.no_tel_mere,   
         patient.nom_pere,   
         patient.no_tel_pere,   
         traitements.traitement_id,   
         patient.referant_nom,   
         patient.new_dossier,   
         patient.no_dossier,   
         traitements.debut_traitement,   
         patient.no_boite,
			(select salutation from t_salutation where patient.id_sal = t_salutation.id_sal) as salutation,
			(select t_salutation.salutation + ' ' + t_referents.ref_prenom + ' ' + t_referents.ref_nom from t_referents inner join t_salutation on t_referents.id_sal = t_salutation.id_sal where id = patient.referant_nom) as refnom
    FROM patient,   
         traitements,   
         ortho_id  
   WHERE patient.patient_id = traitements.patient_id and  
         patient.no_ortho = ortho_id.ortho_id and  
         patient.patient_id = :ra_patient AND  
         traitements.traitement_id = :traitement_id  
" arguments=(("ra_patient", number),("traitement_id", number)) )
