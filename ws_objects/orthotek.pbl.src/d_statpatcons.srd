 column=(type=date updatewhereclause=yes name=factures_traitements_datefact dbname="factures_traitements.datefact" dbalias=".datefact" )
 column=(type=char(100) updatewhereclause=yes name=ortho_id_dr_nom_complet dbname="ortho_id.dr_nom_complet" dbalias=".dr_nom_complet" )
 column=(type=char(50) updatewhereclause=yes name=patient_patient_nom dbname="patient.patient_nom" dbalias=".patient_nom" )
 column=(type=char(50) updatewhereclause=yes name=patient_patient_prenom dbname="patient.patient_prenom" dbalias=".patient_prenom" )
 column=(type=long updatewhereclause=yes identity=yes name=patient_idpat dbname="patient.idpat" dbalias=".idpat" )
 column=(type=char(10) updatewhereclause=yes name=t_status_code dbname="t_status.code" dbalias=".code" )
 column=(type=date updatewhereclause=yes name=rdvdate dbname="rdvdate" )
 column=(type=long updatewhereclause=yes name=nbrdv dbname="nbrdv" )
 column=(type=date updatewhereclause=yes name=rappeldate dbname="rappeldate" )
 retrieve="  SELECT factures_traitements.date_facture ,
			factures_traitements.date_facture as datefact,
		   ortho_id.dr_nom_complet ,
		   patient.patient_nom ,
		   patient.patient_prenom ,
			patient.patient_id as idpat,
		   t_status.code ,
			(select first rdvdate from t_rdv where patient_id = idpat and rdvdate > datefact) as rdvdate,
			(select count(*) from t_rdv where patient_id = idpat) as nbrdv,
			(select first prochain_rv from rendezvous where patient_id = idpat and prochain_rv > datefact) as rappeldate
    FROM factures_traitements ,
		   ortho_id ,
		   patient ,
		   traitements ,
		   t_status     
   WHERE patient.patient_id = traitements.patient_id and
         factures_traitements.traitement_id = traitements.traitement_id and
         factures_traitements.status_id = t_status.status_id and
         ortho_id.ortho_id = patient.no_ortho and
         ortho_id.ortho_id = :ra_ortho_id and
         factures_traitements.date_facture >= :ra_debut and
         factures_traitements.date_facture <= :ra_fin and
         (t_status.status_id = :ra_acte or :ra_acte is null)
ORDER BY patient.patient_nom ASC,
		   patient.patient_prenom ASC,
			factures_traitements.date_facture ASC" arguments=(("ra_ortho_id", number),("ra_debut", date),("ra_fin", date),("ra_acte", number)) )
