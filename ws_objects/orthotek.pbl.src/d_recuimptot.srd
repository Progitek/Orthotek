 retrieve="  SELECT ortho_id.ortho_id,   
         ortho_id.dr_nom_complet,   
         ortho_id.dr_ville_province_code_postale,   
         ortho_id.dr_tel_bur,   
         ortho_id.dr_adresse,
         ortho_id.dr_numero,
			ortho_id.tps,
			ortho_id.tvq, 
			ortho_id.dr_special,
			ortho_id.cie,
         patient.patient_nom,   
         patient.patient_prenom,
			patient.prochain_rappel,
			isnull(patient.aposter,0) as poste,
			patient.recuresp,
			patient.nomresp1,
			patient.langue,
			isnull(patient.new_dossier,'') as new_dossier,  
		   t_salutation.salutation,
         paiement.montant_recu,   
         paiement.date_debut_recu,   
         paiement.paiement_id,   
         paiement.date_paiement,   
			paiement.nomresp,     
         paiement.recu,  
			paiement.recunote,
         paiement.id_contact,
			paiement.taxe,     
         patient.patient_id,
			t_options.recure as re,
			t_options.recudelassnumber as hidenumber,
			isnull(sigrecu,0) as signature,
			t_options.noterecu as recunotefr,
			t_options.noterecuan as recunotean,
			isnull(phasetype,'f') as phasetype,
			t_options.signfile,
			ifnull(t_contact.id_contact,patient.adresse,t_contact.adresse) as adresse,
			ifnull(t_contact.id_contact,patient.ville,t_contact.ville) as ville,
			ifnull(t_contact.id_contact,patient.province,t_contact.province) as province,
			ifnull(t_contact.id_contact,patient.code_postale,t_contact.zip) as codepostal
    FROM patient INNER JOIN ortho_id ON patient.no_ortho = ortho_id.ortho_id
					  INNER JOIN traitements ON traitements.patient_id = patient.patient_id  
          		  INNER JOIN paiement ON traitements.traitement_id = paiement.traitement_patient_id
					  INNER JOIN t_options ON ortho_id.ortho_id = t_options.ortho_id
					  LEFT OUTER JOIN t_contact ON t_contact.id_contact = paiement.id_contact
				     LEFT OUTER JOIN t_salutation ON t_salutation.id_sal = patient.id_sal
   WHERE paiement.paiement_id = :ra_paieid
" arguments=(("ra_paieid", number)) )
