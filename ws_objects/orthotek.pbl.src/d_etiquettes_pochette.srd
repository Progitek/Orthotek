 retrieve="SELECT  patient.patient_nom ,
        patient.patient_prenom ,
        patient.tel_bureau ,
        t_salutation.salutation,
		  t_referents.ref_nom,
		  t_referents.ref_prenom,
        patient.tel_maison ,
        patient.new_dossier,
		  (select salutation from t_salutation where patient.id_sal = t_salutation.id_sal) as salutation
   FROM patient INNER JOIN t_referents ON patient.referant_nom = t_referents.id
    		       INNER JOIN t_salutation ON t_referents.id_sal = t_salutation.id_sal
  WHERE patient.patient_id = :ra_patient " arguments=(("ra_patient", number)) )
