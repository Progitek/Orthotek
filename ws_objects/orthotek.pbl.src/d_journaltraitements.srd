 column=(type=decimal(2) updatewhereclause=yes name=sumfact dbname="sumfact" )
 column=(type=decimal(2) updatewhereclause=yes name=sumpaie1 dbname="sumpaie1" )
 column=(type=decimal(2) updatewhereclause=yes name=sumpaie dbname="sumpaie" )
 column=(type=date updatewhereclause=yes name=maxdatepaie dbname="maxdatepaie" )
 retrieve="SELECT 	patient.patient_nom,
		  	patient.patient_prenom,
		  	ortho_id.dr_nom_complet,
     		traitements.debut_traitement,
			traitements.traitement_id as trait,
			(select sum(montant) from factures_traitements where traitement_id = trait and affect = 1) as sumfact1,
			(if sumfact1 is null then 0 else sumfact1 endif) as sumfact,
			(select sum(montant_recu) from paiement where traitement_patient_id = trait and affect = 1) as sumpaie1,
			(if sumpaie1 is null then 0 else sumpaie1 endif) as sumpaie,
			(select max(date_paiement) from paiement where traitement_patient_id = trait) as maxdatepaie
	FROM 	patient INNER JOIN ortho_id ON ortho_id.ortho_id = patient.no_ortho
		  			  INNER JOIN traitements ON patient.patient_id = traitements.patient_id
  WHERE	ortho_id.ortho_id = :ra_ortho and 
         year(maxdatepaie) = :ra_year and
			year(traitements.debut_traitement) = :ra_year and
			(sumfact - sumpaie) = 0" arguments=(("ra_ortho", number),("ra_year", number)) )
