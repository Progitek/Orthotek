 column=(type=date updatewhereclause=yes name=nextrap dbname="nextrap" )
 retrieve="select t_rdv.rdvdate,
       (patient.patient_prenom + ' ' + patient.patient_nom) as patnom,
       patient.statut,
		 t_dentists.id_dentist,
       t_dentists.det_prenom,
		 t_dentists.det_nom,
		 t_salutation.salutation,
	    (select phase from t_phase where t_phase.id_phase = traitements.id_phase) as idphase,
       (select max(rdvdate) from t_rdv where patient.patient_id = t_rdv.patient_id and rdvdate < today()) as dernrdv,
	    (select min(rdvdate) from t_rdv where patient.patient_id = t_rdv.patient_id and rdvdate > today()) as nextrdv,
		 (select min(prochain_rv) from rendezvous where patient.patient_id = rendezvous.patient_id and prochain_rv > today()) as nextrap
from   t_rdv INNER JOIN patient ON patient.patient_id = t_rdv.patient_id
			    INNER JOIN type_de_traitement ON t_rdv.typetraitid = type_de_traitement.type_traitement_id,
       patient INNER JOIN t_dentists ON t_dentists.id_dentist= patient.id_dentist
					INNER JOIN traitements ON patient.patient_id = traitements.patient_id
					LEFT OUTER JOIN t_salutation ON t_dentists.id_sal = t_salutation.id_sal
where  rdvdate between :ra_start AND :ra_end AND
       patient.no_ortho = :ra_ortho AND
		 t_dentists.actif = 1 AND
		 type_de_traitement.typetrait = 1 AND
		 traitements.phase = 1 AND
		 isnull(t_rdv.rdvetat,'') not in ('c','m')
order by t_dentists.det_nom asc,
			t_dentists.det_prenom asc" arguments=(("ra_ortho", number),("ra_start", date),("ra_end", date)) )
