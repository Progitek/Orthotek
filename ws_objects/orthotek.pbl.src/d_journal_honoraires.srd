 column=(type=char(50) updatewhereclause=yes name=patient_patient_prenom dbname="patient.patient_prenom" dbalias=".patient_prenom" )
 column=(type=long updatewhereclause=yes name=patient_id_satellite dbname="patient.id_satellite" dbalias=".id_satellite" )
 column=(type=char(30) updatewhereclause=yes name=ortho_id_dr_nom_complet dbname="ortho_id.dr_nom_complet" dbalias=".dr_nom_complet" )
 retrieve="  SELECT factures_traitements.date_facture ,
		   factures_traitements.montant ,
			(select description from t_status where t_status.status_id = factures_traitements.status_id) as desctrait ,
		   patient.patient_nom ,
		   patient.patient_prenom ,
			patient.id_satellite,
		   ortho_id.dr_nom_complet
    FROM factures_traitements ,
		   patient ,
		   ortho_id ,
		   traitements     
   WHERE ortho_id.ortho_id = patient.no_ortho and 
         patient.patient_id = traitements.patient_id and
         factures_traitements.traitement_id = traitements.traitement_id and
         ortho_id.ortho_id = :ra_ortho and
         factures_traitements.date_facture >= :ra_debut and
         factures_traitements.date_facture <= :ra_fin and
			(factures_traitements.id_satellite = :ra_idsat or :ra_idsat is null) and
         factures_traitements.cache = 0  
ORDER BY factures_traitements.date_facture ASC  " arguments=(("ra_ortho", number),("ra_debut", date),("ra_fin", date),("ra_idsat", number)) )
