text(band=detail alignment="0" text="durant un mois particulier, le même paiement doit être effectué (vous devez inscrire le nom du patient au bas des chèques pour" border="0" color="33554432" x="64" y="3944" height="56" width="2798" html.valueishtml="0"  name=t_37 visible="1"  font.face="Arial" font.height="-8" font.weight="400"  font.family="2" font.pitch="2" font.charset="0" background.mode="1" background.color="536870912" )
