 retrieve="  SELECT patient.no_ortho,   
         patient.patient_id, 
			patient.date_naissance,    
         patient.new_dossier,   
         patient.langue,   
         patient.patient_prenom,   
         patient.patient_nom  
    FROM patient (index i_pat)
   WHERE patient.no_ortho = :ra_ortho   
           
ORDER BY patient.patient_nom ASC,   
         patient.patient_prenom ASC   
" update="patient" updatewhere=0 updatekeyinplace=no arguments=(("ra_ortho", number)) )
