 column=(type=long updatewhereclause=yes identity=yes name=t_dentists_id_dentist dbname="t_dentists.id_dentist" dbalias=".id_dentist" )
 column=(type=char(30) updatewhereclause=yes name=t_dentists_det_prenom dbname="t_dentists.det_prenom" dbalias=".det_prenom" )
 column=(type=char(30) updatewhereclause=yes name=t_dentists_det_nom dbname="t_dentists.det_nom" dbalias=".det_nom" )
 column=(type=char(50) updatewhereclause=yes name=t_salutation_salutation dbname="t_salutation.salutation" dbalias=".salutation" )
 column=(type=char(50) updatewhereclause=yes name=idphase dbname="idphase" )
 column=(type=date updatewhereclause=yes name=dernrdv dbname="dernrdv" )
