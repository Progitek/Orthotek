table(column=(type=long update=yes updatewhereclause=yes key=yes name=id_lot dbname="t_lot.id_lot" dbalias=".id_lot" )
 column=(type=char(10000) update=yes updatewhereclause=yes name=description dbname="t_lot.description" dbalias=".description" )
 column=(type=long update=yes updatewhereclause=yes key=yes name=ortho_id dbname="t_lot.ortho_id" dbalias=".ortho_id" )
 column=(type=date updatewhereclause=yes name=lotdate dbname="t_lot.lotdate" dbalias=".lotdate" )
