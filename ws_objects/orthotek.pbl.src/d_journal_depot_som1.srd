 column=(type=char(20) updatewhereclause=yes name=ortho_id_dr_compte dbname="ortho_id.dr_compte" )
 column=(type=char(30) updatewhereclause=yes name=ortho_id_dr_banque dbname="ortho_id.dr_banque" )
 retrieve="  SELECT ortho_id.dr_nom_complet,   
         paiement.date_paiement,   
         paiement.montant_recu,   
         paiement.paiement,   
         patient.no_ortho,   
         patient.patient_prenom,   
         patient.patient_nom,   
         ortho_id.dr_compte,   
         ortho_id.dr_banque  
    FROM ortho_id,   
         paiement,   
         patient,   
         traitements  
   WHERE patient.patient_id = traitements.patient_id AND  
         paiement.traitement_patient_id = traitements.traitement_id AND  
         ortho_id.ortho_id = patient.no_ortho AND  
         ortho_id.ortho_id = :ra_no_ortho AND  
         paiement.date_paiement BETWEEN :ra_debut AND :ra_fin AND  
         paiement.cache <> 1  
ORDER BY paiement.paiement ASC,   
         patient.patient_nom ASC,   
         patient.patient_prenom ASC   
" arguments=(("ra_no_ortho", number),("ra_debut", date),("ra_fin", date)) )
