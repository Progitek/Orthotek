 retrieve="SELECT     t_entente.traitement ,
           t_entente.debut ,
           t_entente.fin ,
           t_entente.cout_total ,
           t_entente.cout_total1 ,
           t_entente.cout_total2 ,
           t_entente.cout_total3 ,
           t_entente.diag_anal1 ,
           t_entente.diag_anal2 ,
           t_entente.diag_anal3 ,
           t_entente.paiement_init1 ,
           t_entente.paiement_init2 ,
           t_entente.paiement_init3 ,
           t_entente.paiement_fin1 ,
           t_entente.paiement_fin2 ,
           t_entente.paiement_fin3 ,
           t_entente.mensualite1 ,
           t_entente.mensualite2 ,
           t_entente.mensualite3 ,
           t_entente.duree1,
           t_entente.duree2,
			  t_entente.duree3,
           t_entente.note1,
			  t_entente.estduree1,
			  t_entente.estduree2,
           ortho_id.dr_nom_complet ,
           ortho_id.dr_ville_province_code_postale ,
           ortho_id.dr_tel_bur ,
           ortho_id.dr_adresse ,
           patient.patient_nom ,
           patient.patient_prenom ,
           patient.adresse ,
           patient.ville ,
           patient.province ,
           patient.code_postale ,
           patient.date_naissance     
      FROM patient INNER JOIN t_entente ON t_entente.patient_id = patient.patient_id
                   INNER JOIN ortho_id ON patient.no_ortho = ortho_id.ortho_id 
     WHERE patient.patient_id = :ra_patid  " arguments=(("ra_patid", number)) )
