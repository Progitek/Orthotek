 column=(type=char(50) updatewhereclause=yes name=patient_prenom dbname="patient.patient_prenom" dbalias=".patient_prenom" )
 column=(type=char(100) updatewhereclause=yes name=dr_nom_complet dbname="ortho_id.dr_nom_complet" dbalias=".dr_nom_complet" )
 column=(type=long updatewhereclause=yes identity=yes name=trait dbname="traitements.trait" dbalias=".trait" )
 column=(type=decimal(2) updatewhereclause=yes name=montantpaye dbname="montantpaye" )
 column=(type=decimal(2) updatewhereclause=yes name=montantfacture dbname="montantfacture" )
 retrieve="SELECT  patient.patient_nom ,
        patient.patient_prenom ,
        ortho_id.dr_nom_complet ,
        traitements.traitement_id as trait ,
        (select isnull(sum(isnull(montant_recu,0)),0) FROM paiement WHERE traitement_patient_id = trait) as montantpaye,
        (select isnull(sum(isnull(montant,0)),0) FROM factures_traitements WHERE traitement_id = trait and date_facture <= today())as montantfacture   
FROM    ortho_id INNER JOIN patient ON ortho_id.ortho_id = patient.no_ortho
	              INNER JOIN traitements ON patient.patient_id = traitements.patient_id

WHERE   ortho_id.ortho_id = :ra_orthoid AND 
	     (isnull(montantfacture,0) - isnull(montantpaye,0)) > 0
ORDER BY patient.patient_nom          ASC,
	      patient.patient_prenom       ASC" arguments=(("ra_orthoid", number)) )
